{
  "name": "mailejoe-api",
  "version": "0.1.0",
  "description": "Backend REST API for Mailejoe",
  "author": "Marc Fisher",
  "license": "MIT",
  "keywords": [
    "devops",
    "email",
    "test",
    "mail api",
    "ses"
  ],
  "scripts": {
    "build": "npx rimraf dist && npx tsc -p tsconfig.build.json",
    "format": "npx prettier --write \\\\\\\"src/**/*.ts\\\\\\\"",
    "start": "NODE_ENV=dev npx nodemon --exec ts-node --files src/main.ts",
    "start:test": "NODE_ENV=test npx nodemon --exec ts-node --files src/main.ts",
    "lint": "npx eslint ./**/*.ts",
    "compile:migrations": "mkdir -p .compiled-migrations && npx tsc -outDir ./.compiled-migrations ./migrations/*.ts",
    "migration:create": "npm run typeorm:migrations -- migration:create -n ",
    "migration:run": "npm run typeorm:migrations -- migration:run",
    "migration:show": "npm run typeorm:migrations -- migration:show",
    "migration:revert": "npm run typeorm:migrations -- migration:revert",
    "seed:create": "npm run typeorm:seeds -- migration:create -n ",
    "seed:run": "npm run typeorm:seeds -- migration:run",
    "seed:show": "npm run typeorm:seeds -- migration:show",
    "seed:revert": "npm run typeorm:seeds -- migration:revert",
    "test-ci": "start-server-and-test start:test http://localhost:4500/ping test",
    "test": "NODE_ENV=test npx jest --detectOpenHandles",
    "test:watch": "NODE_ENV=test npx jest --watch",
    "test:cov": "NODE_ENV=test npx jest --coverage",
    "typeorm:migrations": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config config/dev/ormconfig.json",
    "typeorm:seeds": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config config/dev/ormconfig.seeds.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.37.0",
    "@aws-sdk/client-secrets-manager": "^3.37.0",
    "@aws-sdk/client-ses": "^3.37.0",
    "@casl/ability": "^3.4.0",
    "@hapi/boom": "^9.1.4",
    "@hapi/hapi": "^20.2.0",
    "@hapi/joi": "^17.1.1",
    "accept-language-parser": "^1.5.0",
    "accepts": "^1.3.7",
    "axios": "^0.19.2",
    "change-case": "^4.1.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "cls-hooked": "^4.2.2",
    "dotenv": "~8.1.0",
    "i18n": "^0.8.6",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "reflect-metadata": "~0.1.13",
    "request-context": "~2.0.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.13",
    "striptags": "^3.1.1",
    "typeorm": "^0.2.37",
    "typescript": "^3.8.3",
    "ua-parser-js": "^0.7.21",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.144",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.20.27",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "aws-sdk-client-mock": "^0.5.5",
    "axios-logger": "^2.3.1",
    "chance": "^1.1.8",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "jest": "^24.9.0",
    "nodemon": "^2.0.0",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.11.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "~1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "vuepress": "^1.4.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  }
}
