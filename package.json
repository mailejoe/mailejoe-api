{
  "name": "mailejoe-api",
  "version": "0.1.0",
  "description": "Backend REST API for Mailejoe",
  "author": "Marc Fisher",
  "license": "MIT",
  "keywords": [
    "devops",
    "email",
    "test",
    "mail api",
    "ses"
  ],
  "scripts": {
    "build": "npx rimraf dist && npx tsc -p tsconfig.build.json",
    "format": "npx prettier --write \\\\\\\"src/**/*.ts\\\\\\\"",
    "start": "cross-env NODE_ENV=dev npx nodemon --exec ts-node --files src/main.ts",
    "start:test": "cross-env NODE_ENV=test npx nodemon --exec ts-node --files src/main.ts",
    "lint": "npx eslint ./src/**/*.ts",
    "migration:create": "npm run typeorm:migrations -- migration:create -n ",
    "migration:run": "npm run typeorm:migrations -- migration:run",
    "migration:show": "npm run typeorm:migrations -- migration:show",
    "migration:revert": "npm run typeorm:migrations -- migration:revert",
    "test-ci": "start-server-and-test start:test http://localhost:4500/ping test",
    "test": "cross-env NODE_ENV=test npx jest --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test npx jest --watch",
    "test:cov": "cross-env NODE_ENV=test npx jest --coverage",
    "typeorm:migrations": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config config/dev/ormconfig.json",
    "typeorm:seeds": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config config/dev/ormconfig.seeds.json"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.38.0",
    "@aws-sdk/client-s3": "^3.37.0",
    "@aws-sdk/client-secrets-manager": "^3.37.0",
    "@aws-sdk/client-ses": "^3.37.0",
    "@aws-sdk/client-sesv2": "^3.38.0",
    "@casl/ability": "^3.4.0",
    "accept-language-parser": "^1.5.0",
    "accepts": "^1.3.7",
    "axios": "^0.23.0",
    "bcrypt": "^5.0.1",
    "change-case": "^4.1.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.10.2",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "dotenv": "~8.1.0",
    "express": "^4.17.1",
    "follow-redirects": ">=1.14.8",
    "helmet": "^4.6.0",
    "i18n": "^0.8.6",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "lodash": "^4.17.15",
    "luxon": "^2.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "reflect-metadata": "~0.1.13",
    "request-context": "~2.0.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.13",
    "speakeasy": "^2.0.0",
    "striptags": "^3.1.1",
    "typeorm": "^0.2.37",
    "typescript": "^3.8.3",
    "uuid": "^3.3.3",
    "validator": "^13.7.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/helper-compilation-targets": "^7.16.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.144",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.20.27",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "aws-sdk-client-mock": "^0.5.5",
    "axios-logger": "^2.3.1",
    "chance": "^1.1.8",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
